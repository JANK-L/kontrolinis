<%- include('partials/header'); -%>


<h1 class="title">Programuotojai API</h1>

<div id="homepage">
  <%- include('partials/navigation'); -%>
  <h2>Koreguoti programuotojo informacija</h2>
  <div id="devs">
    <div class="dev-container">

      <div class="dev-container">

        <form data-id="<%= dev._id %>">
          <label>Vardas: </label>
          <input type="text" placeholder="Vardas" name="vardas" value="<%= dev.vardas%>" required>

          <label>Technologijos: </label>
          <input type="text" placeholder="Technologijos" name="tech" value="<%= dev.tech.join(' ,')%>" required>

          <label>laisvas: </label>
          <select name="laisvas">
            <option value="true">TAIP</option>
            <option value="false">NE</option>
          </select>

          <label>Koordinates: </label>
          <label>Ilguma: </label>
          <input type="number" placeholder="Ilguma" name="ilguma" step="any" value="<%= dev.location.coordinates[0]%>" required>
          <label>platuma: </label>
          <input type="number" placeholder="platuma" name="platuma" step="any" value="<%= dev.location.coordinates[1]%>" required>

          <input type="submit" value="Atnaujinti">

        </form>

      </div>

    </div>

  </div>
</div>

<script src="/js/put.js"></script>



<%- include('partials/footer'); -%>